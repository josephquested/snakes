    "clone": "^2.1.0",
    "morphdom": "^2.2.1",
    "redux": "^3.6.0",
    "yo-yo": "^1.3.1",
    "browserify": "^13.0.0",
    "gh-pages": "^0.11.0",
    "uglifyify": "^3.0.1"
  }
